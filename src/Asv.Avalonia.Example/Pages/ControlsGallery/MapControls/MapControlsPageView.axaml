<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:map="clr-namespace:Asv.Avalonia.GeoMap;assembly=Asv.Avalonia.GeoMap"
             xmlns:example="clr-namespace:Asv.Avalonia.Example"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="example:MapControlsPageViewModel"
             x:Class="Asv.Avalonia.Example.MapControlsPageView">
    <Design.DataContext>
        <example:MapControlsPageViewModel />
    </Design.DataContext>
    <Panel>
        <map:MapView DataContext="{Binding MapViewModel}" />
    </Panel>
</UserControl>