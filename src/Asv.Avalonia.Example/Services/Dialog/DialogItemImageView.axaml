<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:example="clr-namespace:Asv.Avalonia.Example"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Asv.Avalonia.Example.DialogItemImageView"
             x:DataType="example:DialogItemImageViewModel">
    <Design.DataContext>
        <example:DialogItemImageViewModel />
    </Design.DataContext>
    <Image Source="{Binding Image}" Width="350"/>
</UserControl>
