    <!-- ====================== STATUS BRUSHES ====================== -->
    <SolidColorBrush
        x:Key="AsvForegroundErrorBrush"
        Color="{DynamicResource AsvForegroundErrorColor}"
    />
    <SolidColorBrush
        x:Key="AsvBackgroundErrorBrush"
        Color="{DynamicResource AsvBackgroundErrorColor}"
    />

