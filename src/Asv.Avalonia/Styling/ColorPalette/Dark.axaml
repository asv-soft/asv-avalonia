<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
>
    <!-- Status colors -->
    <Color x:Key="AsvForegroundErrorColor">#cc5058</Color>
    <!-- Red – Error -->
    <Color x:Key="AsvBackgroundErrorColor">#3d353a</Color>

    <Color x:Key="AsvForegroundWarningColor">#e48f4d</Color>
    <!-- Orange – Warning -->
    <Color x:Key="AsvBackgroundWarningColor">#423b3c</Color>

    <Color x:Key="AsvForegroundSuccessColor">#21c088</Color>
    <!-- Green – Success -->
    <Color x:Key="AsvBackgroundSuccessColor">#37453d</Color>

    <Color x:Key="AsvForegroundUnknownColor">Gray</Color>
    <Color x:Key="AsvBackgroundUnknownColor">#3d353a</Color>

    <!-- ====================== INFO PALETTE – 20 COLORS (Dark Theme) ====================== -->

    <Color x:Key="AsvForegroundInfo1Color">#607ceb</Color>
    <!-- Deep Sky Blue -->
    <Color x:Key="AsvBackgroundInfo1Color">#313945</Color>

    <Color x:Key="AsvForegroundInfo2Color">#9a5cd2</Color>
    <!-- Purple -->
    <Color x:Key="AsvBackgroundInfo2Color">#383442</Color>

    <Color x:Key="AsvForegroundInfo3Color">#21bfc0</Color>
    <!-- Bright Cyan -->
    <Color x:Key="AsvBackgroundInfo3Color">#364240</Color>

    <Color x:Key="AsvForegroundInfo4Color">#8c73ff</Color>
    <!-- Electric Violet -->
    <Color x:Key="AsvBackgroundInfo4Color">#353346</Color>

    <Color x:Key="AsvForegroundInfo5Color">#5c9eff</Color>
    <!-- Light Sky Blue -->
    <Color x:Key="AsvBackgroundInfo5Color">#313945</Color>

    <Color x:Key="AsvForegroundInfo6Color">#b366ff</Color>
    <!-- Bright Purple -->
    <Color x:Key="AsvBackgroundInfo6Color">#3a3346</Color>

    <Color x:Key="AsvForegroundInfo7Color">#4da6ff</Color>
    <!-- Azure Blue -->
    <Color x:Key="AsvBackgroundInfo7Color">#313f46</Color>

    <Color x:Key="AsvForegroundInfo8Color">#d473d4</Color>
